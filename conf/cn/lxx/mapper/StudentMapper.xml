<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.lxx.mapper.StudentMapper">
  <insert id="addStudent" parameterType="cn.lxx.entity.Student">
  	INSERT INTO `mybatis`.`student` (`sname`, `sage`, `sex`) VALUES (#{sname}, #{sage}, #{sex});
  </insert>
</mapper>